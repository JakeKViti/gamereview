<%= render partial: "layouts/errors", locals: { object: @games } %>
</br>

<%= form_for @games do |f| %>
  <%= f.label :title %> 
  <%= f.text_field :title%><br>
  <%= f.label :genre %>
  <%= f.text_field :genre%><br>
  <%= f.label :developer %>
  <%= f.text_field :developer%><br>
  <%= f.label :age, "Age Rating" %>
  <%= f.text_field :age%><br>
  <%= f.label :year_released, "Release Year" %> 
  <%= f.number_field :year_released%><br>
  <%= f.submit "Submit Game" %>
<% end %>